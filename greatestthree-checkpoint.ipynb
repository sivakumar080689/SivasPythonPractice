   "source": [
    "#Write a program to find the greatest of three number\n",
    "\n",
    "#Input three numbers\n",
    "a= 2\n",
    "b=5\n",
    "c=8\n",
    "\n",
    "#Find the greatest number using conditional statements\n",
    "if a>=b and a>=c:\n",
    "    greatest = a\n",
    "elif b>=a and b>=c:\n",
    "    greatest = b\n",
    "else:\n",
    "    greatest = c\n",
    "\n",
    "print(f\"greatest number is {greatest}\")\n",
    "\n"
   ]
